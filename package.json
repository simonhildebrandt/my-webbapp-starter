{
  "name": "my-webapp-starter",
  "version": "1.0.0",
  "description": "Basic Browserify/Babel/React stack",
  "main": "index.js",
  "author": "Simon Hildebrandt <simonhildebrandt@gmail.com>",
  "license": "MIT",
  "scripts": {
    "serve": "live-server --port=9000 dev/",
    "build": "browserify src/index.js -o dev/index.js -t [ babelify ]",
    "watch": "watchify src/index.js -o dev/index.js -t [ babelify ]",
    "build_prod": "browserify src/index.js -o public/index.js -t [ babelify ]"
  },
  "dependencies": {
    "@babel/cli": "^7.11.6",
    "@babel/core": "^7.9.0",
    "@babel/plugin-transform-react-jsx": "^7.9.4",
    "@babel/polyfill": "^7.8.7",
    "@babel/preset-env": "^7.9.5",
    "babelify": "^10.0.0",
    "browserify": "^16.5.1",
    "live-server": "^1.2.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-markdown": "^5.0.3",
    "watchify": "^3.11.1"
  }
}
